<form method="POST">
            <h2> Organization Search Criteria</h2>
            <hr />



            <div class="form-horizontal">




                <div class="form-group">
                    <label for="Name" class="col-md-2"> Name: </label>
                    <div class="col-md-4">
                        <input type="text" id="Name" name="Name" class=" form-control form-group" />
                    </div>


                    <label for="Code" class="col-md-2"> Code: </label>
                    <div class="col-md-4">
                        <input type="text" id="Code" name="Code" class="form-control form-group" />

                    </div>


                    <label for="Address" class="col-md-2"> Address: </label>
                    <div class="col-md-4">
                        <input type="text" id="Address" name="Address" class="form-control form-group" />
                    </div>


                    <label for="ContactNo" class="col-md-2"> Conatact No: </label>
                    <div class="col-md-4">
                        <input type="text" id="ContactNo" name="ContactNo" class="form-control form-group" />
                    </div>

                    <div class="col-md-2 col-md-offset-2">
                        <input type="button" id="SeachOrganizationButton" name="SeachOrganizationButton" value="Search" class="form-control form-group btn btn-info" />
                    </div>
                </div>
                
            </div>
        </form>


        <table id="OrganizationSearchTable" class="table table-striped table-bordered " style="width: 100%">
            <thead>
                <tr>
                    <th class="th-">
                        SL

                    </th>
                    <th class="th-lg">
                        Name
                        <i class="fa fa-sort float-right" aria-hidden="true"></i>
                    </th>
                    <th class="th-lg">
                        Code
                        <i class="fa fa-sort float-right" aria-hidden="true"></i>
                    </th>
                    <th class="th">
                        Trainers
                        <i class="fa fa-sort float-right" aria-hidden="true"></i>
                    </th>
                    <th class="th">
                        Students
                        <i class="fa fa-sort float-right" aria-hidden="true"></i>
                    </th>
                    <th class="th">
                        Action
                        <i class="fa fa-sort float-right" aria-hidden="true"></i>
                    </th>

                </tr>
            </thead>
            <tbody id="OrganizationSearchTableContent"></tbody>
        </table>

